set -ex

DOCUMENT_ID=${DOCUMENT_ID:-0x04f4bbcee9e17fb50627e11537551204ea5e00b1ed198492154b040c28704794}
ATTRIBUTE_KEY=${ATTRIBUTE_KEY:-0xe24e7917d4fcaf79095539ac23af9f6d5c80ea8b0d95c9cd860152bff8fdab17}
ORACLE=${ORACLE:-0x024a8aa03f96809173d3534a04bd695343496056}
seth call $ORACLE "values(bytes32,bytes32)" $DOCUMENT_ID $ATTRIBUTE_KEY
seth send $ORACLE "fetchAttribute(bytes32,bytes32)" $DOCUMENT_ID $ATTRIBUTE_KEY
seth call $ORACLE "values(bytes32,bytes32)" $DOCUMENT_ID $ATTRIBUTE_KEY

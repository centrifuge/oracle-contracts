set -ex

NFT_ID=${NFT_ID:-41655702060068977596055658041433754581741912055812601102818571701136633124578}
ATTRIBUTE_KEY=${ATTRIBUTE_KEY:-0xe24e7917d4fcaf79095539ac23af9f6d5c80ea8b0d95c9cd860152bff8fdab17}
ORACLE=${ORACLE:-0xecfc2059b09ca146d2b09e5f1f377c124ea2bcd4}
LINK=${LINK:-0xa36085F69e2889c224210F603D836748e7dC0088}
NFT_UPDATE=${NFT_UPDATE:-0xbfc931f60a6ef3322ce09d3523481d77d97ff0f9}
ORACLE_PAYMENT=${ORACLE_PAYMENT:-1000000000000000000}
seth send $LINK "approve(address,uint256)(bool)" $ORACLE $ORACLE_PAYMENT
seth send $ORACLE "fetch(uint)" $NFT_ID
echo "Fetching value from Oracle"
seth call $ORACLE "nftData(uint)(uint80,uint128,uint48)" $NFT_ID
echo "Fetching value from NFTUpdate"
seth call $NFT_UPDATE "nftData()(bytes32,uint,uint)"
